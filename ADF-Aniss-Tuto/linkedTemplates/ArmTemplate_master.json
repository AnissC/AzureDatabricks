{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Aniss-Tuto"
		},
		"AzureDatabricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks'"
		},
		"DestinationContainer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DestinationContainer'"
		},
		"WikiDataDestination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'WikiDataDestination'"
		},
		"PublicDataset_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'PublicDataset'"
		},
		"WikiFileREST_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dumps.wikimedia.org/other/pageviews/2019/2019-11/"
		},
		"WikiSourceDS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dumps.wikimedia.org/other/pageviews/2019/2019-11/pageviews-20191101-000000.gz"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks_accessToken": {
						"value": "[parameters('AzureDatabricks_accessToken')]"
					},
					"DestinationContainer_connectionString": {
						"value": "[parameters('DestinationContainer_connectionString')]"
					},
					"WikiDataDestination_connectionString": {
						"value": "[parameters('WikiDataDestination_connectionString')]"
					},
					"PublicDataset_sasUri": {
						"value": "[parameters('PublicDataset_sasUri')]"
					},
					"WikiFileREST_properties_typeProperties_url": {
						"value": "[parameters('WikiFileREST_properties_typeProperties_url')]"
					},
					"WikiSourceDS_properties_typeProperties_url": {
						"value": "[parameters('WikiSourceDS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}