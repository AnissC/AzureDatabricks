{
	"name": "WikiPipeline",
	"properties": {
		"activities": [
			{
				"name": "CopyFromWiki",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "WikiFileREST",
						"type": "DatasetReference",
						"parameters": {
							"dateTRT": "@pipeline().parameters.dateTRT",
							"dateTRT-1": "@pipeline().parameters.dateTRT-1"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "WikiDateDestination",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}